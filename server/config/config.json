{
    "website": {
        "name": "MFA mal anders",
        "noreplyEmail": "noreply@mfa-mal-anders.de",
        "contactEmail": "kontakt@mfa-mal-anders.de",
        "emailFrom": "MFA mal anders",
        "newsletterSuccessPath": "/page/subscription-successful",
        "newsletterUnsubscribeSuccessPath": "/page/unsubscribed-successful"
    },
    "stripe": {
        "paymentMethods": ["card", "giropay"],
        "currency": "eur",
        "pricePerJob": 2500,
        "paymentExpirationDays": 60,
        "refreshFrequencies": [
            {
                "amount": 12000,
                "refreshAfterDays": 14
            }
        ]
    },
    "redirect": {
