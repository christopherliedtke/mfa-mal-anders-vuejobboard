    },
    "prerender": {
        "active": true,
        "exclude": [
            "googlebot",
            "bingbot",
            "rogerbot",
            "embedly",
            "showyoubot",
            "outbrain",
            "vkShare",
            "tumblr",
            "Google Page Speed",
            "Qwantify",
            "yandex",
            "Yahoo! Slurp",
            "baiduspider",
            "bitlybot",
            "Bitrix link preview",
            "Chrome-Lighthouse"
        ]
    },
    "googleIndexing": {
        "active": true,
        "pathPrefix": "/jobboard/job/"
    },
    "facebook": {
        "autoPost": false
    },
    "newsletter": {
        "active": false,
        "interval": "*/10 * * * * *",
        "prodInterval": "0 0 18 * * 0"
    },
    "unpublishedJobsReminder": {
        "active": true,
        "interval": "0 0 17 * * *",
        "prodInterval": "0 0 17 * * 1-5"
    },
    "refreshJobs": {
        "active": true,
        "interval": "0 0 0 * * *"
    },
    "unpublishJobs": {
        "active": true,
        "interval": "0 0 0 * * *"
    }
